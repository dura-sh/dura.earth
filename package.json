{
  "name": "dura.earth",
  "version": "0.0.1",
  "description": "site of Dura.*",
  "homepage": "https://dura.earth/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dura-sh/dura.earth"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:azure": "npm install --arch=x64 --platform=linuxmusl sharp",
    "dev": "rm -rf .cache public && gatsby develop",
    "docker:dev": "docker-compose -f ./docker/docker-compose.dev.yml up",
    "docker:down": "docker-compose -f ./docker/docker-compose.dev.yml down",
    "docker:push": "npm run docker:hub:build && npm run docker:tag && docker push durash/earth",
    "docker:prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker:prod:dev": "docker-compose -f ./docker/docker-compose.yml up --build",
    "docker:build": "docker-compose -f ./docker/docker-compose.dev.yml build",
    "docker:tag": "docker tag dura-earth durash/earth",
    "docker:hub:build": "docker build  -f Dockerfile.hub -t  durash/earth ./",
    "lint": "npm run lint:es && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint:es": "eslint ./ --ext '.js,.jsx'",
    "lint:style": "stylelint \"site/**/*.less\" --syntax less",
    "precommit": "npm run lint-staged",
    "prettier": "prettier --write ./site/**/**/**",
    "serve": "gatsby serve",
    "site": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npm run build",
    "start": "npm run dev",
    "test": "npm run lint"
  },
  "pre-commit": [
    "precommit"
  ],
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "dependencies": {
    "@types/react-document-title": "^2.0.3",
    "@types/react-intl": "^2.3.17",
    "antd": "3.16.2",
    "applicationinsights": "^1.4.0",
    "bizcharts": "^3.5.4",
    "detect-libc": "^1.0.3",
    "docsearch.js": "^2.6.3",
    "gatsby-plugin-google-analytics": "^2.1.1",
    "gatsby-plugin-sitemap": "^2.2.1",
    "himalaya": "^1.1.0",
    "ink": "^2.3.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.5.0",
    "prop-types": "^15.7.2",
    "rc-drawer": "^1.10.1",
    "react": "^16.8.6",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-github-button": "^0.1.11",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.4.0",
    "react-media": "^1.9.2",
    "semver": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/generator": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/traverse": "^7.0.0",
    "acorn": "^6.1.1",
    "better-queue": "^3.8.10",
    "classnames": "^2.2.5",
    "commander": "~2.17.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.1",
    "debug": "^4.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "eslint-tinker": "^0.5.0",
    "fs-extra": "^7.0.0",
    "gatsby": "^2.19.10",
    "gatsby-plugin-antd": "^2.0.2",
    "gatsby-plugin-catch-links": "^2.0.0",
    "gatsby-plugin-less": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-remark-copy-linked-files": "^2.1.0",
