    "less": "^3.8.1",
    "lint-staged": "^7.0.5",
    "mdast-util-to-string": "^1.0.5",
    "mini-css-extract-plugin": "^0.7.0",
    "npm-run-all": "^4.0.2",
    "nprogress": "^0.2.0",
    "parallelshell": "^3.0.1",
    "postcss": "^7.0.17",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.0",
    "prismjs": "^1.15.0",
    "rc-queue-anim": "^1.3.1",
    "rc-scroll-anim": "^2.0.2",
