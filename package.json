    "shelljs": "^0.8.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-standard": "^18.2.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2"
  },
  "title": "Dura.*",
  "engines": {
    "npm": "<=10.16.0",
    "node": "<=6.9.0"
  }
}
